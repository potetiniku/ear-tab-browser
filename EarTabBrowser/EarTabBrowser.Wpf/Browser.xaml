<UserControl
	x:Class="EarTabBrowser.Wpf.Browser"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:EarTabBrowser.Wpf"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:webview="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<DockPanel LastChildFill="True">
		<Grid DockPanel.Dock="Top">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="Auto"/>
				<ColumnDefinition Width="Auto"/>
				<ColumnDefinition Width="Auto"/>
				<ColumnDefinition Width="*"/>
				<ColumnDefinition Width="Auto"/>
				<ColumnDefinition Width="Auto"/>
			</Grid.ColumnDefinitions>
			<Button
				Grid.Column="0"
				Content="前へ"/>
			<Button
				Grid.Column="1"
				Content="次へ"/>
			<Button
				Grid.Column="2"
				Content="更新"/>
			<TextBox
				Grid.Column="3"
				Text="hoge"/>
			<Button
				Grid.Column="4"
				Content="最大化"/>
			<Button
				Grid.Column="5"
				Content="閉じる"/>
		</Grid>
		<webview:WebView2 Source="https://google.com"/>
	</DockPanel>
</UserControl>
